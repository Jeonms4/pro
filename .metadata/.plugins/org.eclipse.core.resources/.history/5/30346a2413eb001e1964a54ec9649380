package sec02;

import lombok.AllArgsConstructor;
import lombok.Data;

public class Student implements Comparable<T>{
	private String name;
	private int score;
	
	public Student(String name, int score) {
		super();
		this.name = name;
		this.score = score;
	}
	
	public int compareTO(Student stu) {
		return Integer.compare(this.score, stu.score);
	}
}
