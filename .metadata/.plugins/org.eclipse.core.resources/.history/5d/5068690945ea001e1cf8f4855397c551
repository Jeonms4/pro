package sec01;

import java.util.ArrayList;
import java.util.stream.Stream;

public class StreamEx {

	public static void main(String[] args) {
		ArrayList<Integer> arrList1 = new ArrayList<>();
		arrList1.add(10);
		arrList1.add(7);
		arrList1.add(5);
		arrList1.add(8);
		
		for(int i : arrList1) {
			System.out.println(i);
		}
		
		ArrayList<Integer> arrList2 = (ArrayList<Integer>) arrList1.stream();
		
		Stream stream = arrList1.stream();
	}

}
