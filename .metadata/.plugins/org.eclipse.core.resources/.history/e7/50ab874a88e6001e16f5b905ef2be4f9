package com.spring.ex01;

import lombok.Data;

@Data
public class Student {
	private int ban;
	private String name;
	
	public int getBan() {
		return ban;
	}
	
	public void setBan() {
		this.ban = ban;
	}
	
	public int getName() {
		return name;
	}
	
	public void setBan() {
		this.ban = ban;
	}
	
	public Student(int ban, String name) {
		this.ban = ban;
		this.name = name;
	}
}
